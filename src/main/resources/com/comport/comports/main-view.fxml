<?xml version="1.0" encoding="UTF-8"?>

<?import com.comport.comports.view.NumberTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.TextFlow?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.comport.comports.COMPortsController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <GridPane hgap="10" vgap="10">
        <Label fx:id="pairLabel1" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <TextFlow fx:id="pairLabel12" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <NumberTextField fx:id="pairNumDataBits1" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
        <TextArea fx:id="serialPort1Text1" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
        <TextArea fx:id="serialPort1Text2" GridPane.columnIndex="0" GridPane.rowIndex="4" disable="true" />
        <Button fx:id="sendButton1" GridPane.columnIndex="0" GridPane.rowIndex="5" text="Send"/>

        <Label fx:id="pairLabel2" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <TextFlow fx:id="pairLabel22" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <NumberTextField fx:id="pairNumDataBits2" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <TextArea fx:id="serialPort2Text1" GridPane.columnIndex="1" GridPane.rowIndex="3" disable="true"/>
        <TextArea fx:id="serialPort2Text2" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <Button fx:id="sendButton2" GridPane.columnIndex="1" GridPane.rowIndex="5" text="Send"/>
    </GridPane>
</VBox>
